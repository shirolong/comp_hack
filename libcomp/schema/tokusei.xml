    <object name="Tokusei" persistent="false">
        <member type="s32" name="ID"/>
        <member type="enum" name="TargetType" underlying="int8_t">
            <value>SELF</value>
            <value>PARTNER</value>
            <value>SUMMONER</value>
            <value>PARTY</value>
        </member>
        <member type="list" name="Conditions">
