            <value>PURSUIT_AFFINITY_OVERRIDE</value>
            <value>SKILL_STACK_ADJUST</value>
            <value>SKILL_ITEM_STACK_ADJUST</value>
            <value>STATUS_INFLICT_ADJUST</value>
            <value>WEAPON_AFFINITY_OVERRIDE</value>

